[32m+[m[32m<!DOCTYPE html>[m
[32m+[m[32m<html lang="en">[m
[32m+[m[32m  <head>[m
[32m+[m[32m    <meta charset="UTF-8" />[m
[32m+[m[32m    <title>Teste JavaScript</title>[m
[32m+[m[32m    <script>[m
[32m+[m[32m      var quebrada = false;[m
[32m+[m[32m      function mudaLampada(tipo) {[m
[32m+[m[32m        if (!quebrada) {[m
[32m+[m[32m          document.getElementById("luz").src = "_imagens/" + tipo + ".jpg";[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (tipo == "lampada-quebrada") {[m
[32m+[m[32m          quebrada = true;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    </script>[m
[32m+[m[32m  </head>[m
[32m+[m[32m  <body>[m
[32m+[m[32m    <h1>Acenda a L√¢mpada</h1>[m
[32m+[m[32m    <img[m
[32m+[m[32m      src="_imagens/lampada-apagada.jpg"[m
[32m+[m[32m      id="luz"[m
[32m+[m[32m      onmousemove="mudaLampada('lampada-acesa')"[m
[32m+[m[32m      onmouseout="mudaLampada('lampada-apagada')"[m
[32m+[m[32m      onclick="mudaLampada('lampada-quebrada')"[m
[32m+[m[32m    />[m
